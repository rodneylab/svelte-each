{
  "name": "svelte-each",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "description": "Using Svelte each blocks: how to loop on array-like objects in Svelte + simplify your code using const & style directives and destructuring.",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:css": "stylelint \"src/**/*.{css,svelte}\"",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@fontsource/fira-code": "^4.5.12",
    "@fontsource/open-sans": "^4.5.13",
    "@playwright/test": "^1.27.1",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "commitlint": "^17.1.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "stylelint": "^14.14.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "~4.8.4",
    "vite": "^3.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/svelte-each"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/svelte-each/issues"
  },
  "type": "module",
  "peerDependencies": {
    "@types/node": "^18.7.13",
    "ts-node": "^10.9.1",
    "vite": "^3.0.9"
  }
}
